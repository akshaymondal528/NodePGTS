{
  "name": "nodepgts",
  "version": "1.0.0",
  "description": "Node.js with postgreSQL and typescript",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "build:dev": "tsc && nodemon dist/index.js",
    "start:dev": "nodemon -e ts --exec \"npm run build:dev\""
